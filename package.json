{
  "name": "openai-realtime-twilio-demo",
  "version": "1.0.0",
  "private": true,
  "description": "OpenAI Realtime API + Twilio voice calling integration",
  "repository": {
    "type": "git",
    "url": "https://github.com/openai/openai-realtime-twilio-demo.git"
  },
  "scripts": {
    "install:shared": "cd shared && npm install",
    "install:server": "cd websocket-server && npm install",
    "install:all": "npm run install:shared && npm run install:server",
    "build": "cd websocket-server && npm run build",
    "start": "cd websocket-server && npm start",
    "dev": "cd websocket-server && npm run dev"
  },
  "keywords": [
    "openai",
    "realtime",
    "twilio",
    "voice",
    "websocket"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
